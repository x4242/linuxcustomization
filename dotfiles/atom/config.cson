"*":
  core:
    disabledPackages: [
      "metrics"
      "language-gfm"
      "markdown-preview"
    ]
    telemetryConsent: "no"
  date:
    dateTimeFormat: "YYYY-MM-DDTHH:mm:ssZ"
  editor:
    showIndentGuide: true
  "exception-reporting":
    userId: "f709d414-d4bd-47b6-a062-655aa3848532"
  "line-ending-selector":
    defaultLineEnding: "LF"
  "linter-ui-default":
    showPanel: true
  "markdown-preview-enhanced":
    openPreviewPaneAutomatically: false
    previewTheme: "one-dark.css"
  welcome:
    showOnStartup: false
